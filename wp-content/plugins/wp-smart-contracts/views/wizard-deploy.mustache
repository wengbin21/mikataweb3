<div id="is-wizard-deploy-{{flavor}}" class="wps-full-wrapper light-theme">
    <!-- MAIN WRAPPER -->
    <div class="wps-main-wrapper">
        {{{sidebar}}}
        <!-- WPS CONTENT -->

        <div class="wps-content-page">

            {{{main-nav}}}

            <!-- WSP GRID -->
            <div class="wps-dashboard-grid">

                {{{banner-wizard}}}

                <div class="wps-inner-content">

                    <!-- STEPS -->
                    <div class="wps-steps-container">

                        {{{subheader-step4}}}

                        <p class="wps-step-text" style="margin-bottom: 30px;">{{wps-step-text}}</p>

                        <div class="wps-colBox-title-lg">
                            <img src="{{white-cog}}">{{crypto-options}}
                        </div>
                        <div class="wps-smartContracts-table card">
                            <div id="wps-container-deploy" class="wps-form-box" {{#is_edit}}style="display:none"{{/is_edit}}>
                                <div class="wps-form-container">
                                    {{{form-fields}}}
                                </div>
                                {{#is_edit}}
                                    <a id="wps-update-button" data-flavor="{{flavor}}" data-id="{{id}}" data-account="{{account}}" data-network="{{wpsc_network}}" data-contract="{{wpsc_contract_address}}" href="#" class="wps-cta wps-button-update">UPDATE</a>
                                    <p>&nbsp;</p>
                                    <p class="wps-centered">URL <a href="{{permalink_post}}">{{permalink_post}}</a></p>
                                {{/is_edit}}
                            </div>
                        </div>
                        {{^is_edit}}
                            {{{deploy-section}}}
                        {{/is_edit}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<input id="wps-network" type="hidden" value="{{wpsc_network}}">
<div class="wps-form-col">

    <div class="wsp-form-group">
        <label class="wps-form-label">{{symbol}}</label>
        <p class="wps-form-text-tooltip">{{symbol-desc}}</p>
        <div class="wsp-input-container">
            <input id="wps-symbol" name="symbol" type="text" placeholder="{{symbol-label}}">
            <img src="{{path-to-launcher}}img/key-solid.svg">
        </div>
    </div>

    <div class="wsp-form-group">
        <label class="wps-form-label">{{decimals}}</label>
        <p class="wps-form-text-tooltip">{{decimals-desc}}</p>
        <div class="wsp-input-container">
            <input id="wps-decimals" name="decimals" type="number" min="0" max="18" placeholder="18">
            <img src="{{path-to-launcher}}img/calculator.png">
        </div>
    </div>

    <div class="wsp-form-group">
        <label class="wps-form-label">{{reflection}}</label>
        <p class="wps-form-text-tooltip">
            {{reflection-desc}}
            <span class="wps-form-tooltip-icon">
                <img src="{{path-to-launcher}}img/circle-question-solid.svg">
                <span class="wps-form-tooltip">{{reflection-tip}}</span>
            </span>    
        </p>

        <select id="wps-reflect-fee" placeholder-text="1%" class="select-dropdown__button">
            <option value="100" class="select-dropdown__list-item" selected>1%</option>
            <option value="50"  class="select-dropdown__list-item">2%</option>
            <option value="33"  class="select-dropdown__list-item">3%</option>
            <option value="20"  class="select-dropdown__list-item">5%</option>
            <option value="10"  class="select-dropdown__list-item">10%</option>
            <option value="5"   class="select-dropdown__list-item">20%</option>
        </select>                
    </div>

</div>
<div class="wps-form-col">

    <div class="wsp-form-group">
        <label class="wps-form-label">{{name}}</label>
        <p class="wps-form-text-tooltip">{{name-desc}}
            <span class="wps-form-tooltip-icon">
                <img src="{{path-to-launcher}}img/circle-question-solid.svg">
                <span class="wps-form-tooltip">{{name-tip}}</span>
            </span>
        </p>
        <div class="wsp-input-container">
            <input id="wps-name" name="sales-commissions" type="text" placeholder="{{name-label}}">
            <img src="{{path-to-launcher}}img/symbol-icon.svg">
        </div>
    </div>

    <div class="wsp-form-group">
        <label class="wps-form-label">{{supply}}</label>
        <p class="wps-form-text-tooltip">{{supply-desc}}
            <span class="wps-form-tooltip-icon">
                <img src="{{path-to-launcher}}img/circle-question-solid.svg">
                <span class="wps-form-tooltip">{{supply-tip}}
                </span>
            </span>
        </p>
        <div class="wsp-input-container">
            <input id="wps-supply" name="royalty-creators" type="number" placeholder="1000000">
            <img src="{{path-to-launcher}}img/coins.png">
        </div>
    </div>

</div>

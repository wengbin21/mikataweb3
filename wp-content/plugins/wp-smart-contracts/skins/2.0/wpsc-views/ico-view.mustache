                                        <p class="wps-title-link">{{approved-funds}}</p>
                                        <div class="wps-tag extra content">
                                            <div class="ui image label">
                                                <div class="detail" id="wpsc-approved-funds"></div>
                                            </div>
                                        </div>
                                        <div class="wps-top-spacer"></div>
                                        <div class="wps-tag" style="background-image: none;border: 0;">
                                            <a href="javascript:void(0);" class="wps-cta wps-secondary wps-center wpsc-modal-button" id="wpsc-approve-button">{{approve}}</a>
                                        </div>
                                        <div class="ui hidden divider"></div>                                                                    

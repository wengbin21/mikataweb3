<div id="wpsc-stake-container" class="wps-full-wrapper wps-halpage">
    <div class="wps-content-page">
        <div style="padding: 0 10px">
            {{{main-nav}}}
        </div>
        <div class="wps-dashboard-grid">

            <input type="hidden" id="wpsc-contract-flavor" value="bubblegum">
            <input type="hidden" id="wpsc-contract" value="{{wpsc_contract_address}}">
            <input type="hidden" id="wpsc-network" name="wpsc-nft-network" value="{{network}}">
            <input type="hidden" id="wpsc-ico-token" name="wpsc-ico-token" value="">
            <input type="hidden" id="wpsc-token-symbol" name="wpsc-token-symbol">
            <input type="hidden" id="wpsc-token-name" name="wpsc-token-name">
            <input type="hidden" id="wpsc-token-decimals" name="wpsc-token-decimals">
            <input type="hidden" id="wpsc-native-coin" name="wpsc-native-coin" value="{{wpsc_native_coin}}">
            <input type="hidden" id="wps-remaining-input" name="wps-remaining-input" value="">    
            <input type="hidden" id="wps-rate" name="wps-rate" value="{{rate}}">    
            <input type="hidden" id="wps-etherscan" name="wps-etherscan" value="{{etherscan}}">    

            {{{view-metamask}}}

            <div class="wps-description-box-out-border wps-description-box-out-border-bottom">
                <div class="wps-description-box medium">
                    <img src="{{#page-thumbnail}}{{page-thumbnail}}{{/page-thumbnail}}{{^page-thumbnail}}{{wpsc_blockie}}{{/page-thumbnail}}" class="wps-big-icon medium">
                    <h2 class="wps-box-heading">{{{wpsc_title}}}</h2>
                </div>
            </div>
            {{{wpsc_content}}}
            <div id="wpsc-generic-message"></div>

            {{^hide-brand}}
                <div class="wps-main-wrapper" style="margin-top: 20px;">
                    <div class="wps-center">
                        <span class="wps-tag">
                            <span class="wps-tag-value">
                                <span style="color: {{color}}; margin-right: 10px">⬤</span>
                                {{network_val}}
                            </span>
                        </span>
                        <span class="wps-tag">
                            <span class="wps-tag-value">
                                <span class="wps-bg-{{flavor-color}} color">⬤</span>
                                {{wpsc-flavor}}
                            </span>
                        </span>
                        <div class="wps-tag extra content">
                            <div class="ui labels">
                                <div class="ui image label">
                                    <img src="{{wpsc_blockie}}">
                                    {{smart-contract-label}}
                                    <div class="detail">
                                        <a href="{{etherscan}}address/{{wpsc_contract_address}}"
                                            target="_blank">{{wpsc_contract_address_short}}</a>
                                        <i class="copy icon"
                                            onclick="copyToClipboard('{{wpsc_contract_address}}', 'wpsc-nft-collection-contract')"></i>
                                        <span id="wpsc-nft-collection-contract"></span><span
                                            id="wpsc-copy-msg-wpsc-nft-collection-contract"
                                            style="display: none;">{{copied}}</span><span
                                            id="wpsc-copy-msg-wpsc-nft-collection-contract"
                                            style="display: none;">{{copied}}</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {{/hide-brand}}

            <div class="ui hidden divider"></div>

            <div class="wps-main-wrapper" id="wpsc-crowd-panel" style="display: block">
                <h3 class="wps-colBox-title-lg">
                    💼 &nbsp; {{ico-label}}
                </h3>

                <div class="ui hidden divider"></div>

                <div id="wpsc-requests-container" class="wps-cards">

                    <div class="wps-card">
                        <div class="wps-panel-aaa">
                            <div class="wps-panel wps-panel-content">
                                <div class="wps-column-box wps-column-box-inner" style="margin-top: 0">
                                    <div class="wps-smartContracts-table card wps-table">
                                        <p class="wps-title-link">{{rate-label}}</p>
                                        <div class="wps-tag extra content center">
                                            <div id="wps-rate-detail" class="ui image label"></div>
                                        </div>
                                        <div class="ui hidden divider"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="wps-card">
                        <div class="wps-panel-aaa">
                            <div class="wps-panel wps-panel-content">
                                <div class="wps-column-box wps-column-box-inner" style="margin-top: 0">
                                    <div class="wps-smartContracts-table card wps-table">
                                        <p class="wps-title-link">{{token-for-sale}}</p>
                                        <div class="wps-tag extra content center">
                                            <div class="ui image label" id="wpscContractAddressDiv">
                                                <a id="wpsc_token_link" href="" target="_blank"></a>
                                                <img id="wpsc-add-to-metamask" src="{{fox}}" class="ui mini image"
                                                    data-address="" data-symbol="" data-name=""
                                                    title="Add token to Metamask" data-network="{{network_val}}"
                                                    data-decimals="">
                                                <span id="wpsc-nft-collection-contract-2"></span>
                                            </div>
                                        </div>
                                        <div class="ui hidden divider"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="wps-card">
                        <div class="wps-panel-aaa">
                            <div class="wps-panel wps-panel-content">
                                <div class="wps-column-box wps-column-box-inner" style="margin-top: 0">
                                    <div class="wps-smartContracts-table card wps-table">
                                        <p class="wps-title-link">{{remaining-tokens}}</p>
                                        <div class="wps-tag extra content center">
                                            <div id="wps-remaining" class="ui image label"></div>
                                        </div>
                                        <div class="ui hidden divider"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

                <div id="raised-panel" class="wps-panel-aaa wps-cards-2 wps-huge" style="display:none">
                    <div class="wps-smartContracts-table card wps-table wps-card">
                        <div class="wps-top-spacer"></div>
                        <h3 class="wps-colBox-title-lg">
                            📈 &nbsp; {{raised-label}}
                        </h3>
                        <div class="wps-top-spacer"></div>
                        <div class="wps-tag extra content center">
                            <div class="ui image label">
                                <div class="detail" id="wpsc-raised"></div>
                            </div>    
                        </div>
                    </div>
                    <div class="wps-smartContracts-table card wps-table wps-card">

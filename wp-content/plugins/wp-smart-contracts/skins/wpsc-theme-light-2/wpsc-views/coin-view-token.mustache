{{#contract-exists}}
	  <div class="ui card no-card white-text">

	    <div class="content">

		  <h3 class="ui block header white-header">
		    	<img class="tiny ui circular image" src="{{blockie}}">
				{{{token-name}}} ({{{token-symbol}}})
		  </h3>

	      <div class="extra content">
	  
	        <div class="ui aligned">

			  <div class="ui medium {{color}} label fluid the-btn uppercase margin-bottom left-align">
				  <i class="{{icon}} icon"></i>{{ethereum-network}}
			  </div>
				<div class="clearfix"></div>

			  {{{token-info}}}

	        </div>
	  
	      </div>

	    </div>
	  
	  </div>
{{/contract-exists}}
